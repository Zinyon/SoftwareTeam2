<% layout('layout/layout')%>


<form action="/auth/login_process" method="post">
    <div class = "container1" align = "center">
     <h2 style="font-size: 25px; font-weight: bold; margin-top: 70px; margin-bottom: 10px;">맞춤 시간표 추천: 시추</h2>
     <h3 style="text-indent: 0px; text-align: center; margin: 20px 0px 30px 0px;">시간표의 신세계를 경험하세요</h3>
        <div style="margin-top: 90px;">
           <h3>이메일</h3>
           <input type="text" name="email" placeholder="id@example.com">
           <h3>비밀번호</h3>
           <input type="password" name="pwd" placeholder="password">
           <input type="submit" value="로그인">
        </div>
    </div>
</form>